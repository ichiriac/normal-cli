{
  "name": "normal-cli",
  "version": "1.0.0",
  "description": "The Normal CLI for managing migrations, models, and seeds in Normal ORM projects",
  "main": "bin/run.js",
  "bin": {
    "normal-cli": "./bin/run.js",
    "normal": "./bin/run.js"
  },
  "scripts": {
    "test": "jest --runInBand"
  },
  "keywords": [
    "normal",
    "orm",
    "cli",
    "migrations",
    "database",
    "normaljs",
    "sql",
    "postgresql",
    "mysql",
    "sqlite"
  ],
  "author": "Normal CLI Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ichiriac/normal-cli.git"
  },
  "homepage": "https://github.com/ichiriac/normal-cli#readme",
  "bugs": {
    "url": "https://github.com/ichiriac/normal-cli/issues"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "@oclif/core": "^4.8.0",
    "@oclif/plugin-help": "^6.2.35",
    "chalk": "^4.1.2",
    "fs-extra": "^11.3.2",
    "inquirer": "^12.10.0"
  },
  "devDependencies": {
    "jest": "^29.0.0",
    "knex": "^3.1.0",
    "normaljs": "^1.0.4",
    "sqlite3": "^5.1.7"
  },
  "jest": {
    "testEnvironment": "node",
    "roots": ["<rootDir>/test"]
  },
  "oclif": {
    "bin": "normal-cli",
    "dirname": "normal-cli",
    "commands": "./src/commands",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": ":"
  }
}
